{
  "name": "dlxlib",
  "version": "2.0.0-alpha.1",
  "description": "Solves exact cover problems by implementing Donald E. Knuth's Algorithm X using the Dancing Links technique (DLX)",
  "keywords": [
    "dancing links",
    "dlx",
    "exact cover",
    "algorithm"
  ],
  "main": "dist/dlxlib.js",
  "files": [
    "dist",
    "docs"
  ],
  "directories": {
    "dist": "dist",
    "test": "test"
  },
  "scripts": {
    "eslint": "eslint .",
    "build": "webpack",
    "test": "mocha",
    "docs": "jsdoc --package package.json --readme HOME.md --destination docs --template node_modules/docdash src/dlx.js",
    "pretest": "npm run build",
    "prepublishOnly": "npm test && npm run docs"
  },
  "author": "Jonathan Taylor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorjg/dlxlibjs.git"
  },
  "homepage": "https://github.com/taylorjg/dlxlibjs",
  "bugs": {
    "url": "https://github.com/taylorjg/dlxlibjs/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "core-js": "^3.1.4",
    "deep-equal-in-any-order": "^1.0.13",
    "docdash": "^1.1.1",
    "eslint": "^5.16.0",
    "jsdoc": "^3.6.2",
    "mocha": "^6.1.4",
    "regenerator-runtime": "^0.13.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4"
  },
  "dependencies": {}
}
