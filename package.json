{
  "name": "dlxlib",
  "version": "2.0.0-a",
  "description": "Solves exact cover problems by implementing Donald E. Knuth's Algorithm X using the Dancing Links technique (DLX)",
  "keywords": [
    "dancing links",
    "dlx",
    "exact cover",
    "algorithm"
  ],
  "main": "./lib/index.js",
  "files": [
    "/lib",
    "/doc"
  ],
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "eslint": "eslint .",
    "build": "npm run rollup",
    "rollup": "rollup --config",
    "test": "npm run build && mocha",
    "prepublishOnly": "npm test",
    "doc": "jsdoc src/dlx.js --destination doc --template node_modules/docdash"
  },
  "author": "Jonathan Taylor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorjg/dlxlibjs.git"
  },
  "homepage": "https://github.com/taylorjg/dlxlibjs",
  "bugs": {
    "url": "https://github.com/taylorjg/dlxlibjs/issues"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "docdash": "^1.1.1",
    "eslint": "^5.16.0",
    "jsdoc": "^3.6.2",
    "mocha": "^6.1.4",
    "rollup": "^1.12.4",
    "rollup-plugin-terser": "^5.0.0"
  },
  "dependencies": {}
}
