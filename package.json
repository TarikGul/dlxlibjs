{
  "name": "dlxlib",
  "version": "1.0.3",
  "description": "Solves exact cover problems by implementing Donald E. Knuth's Algorithm X using the Dancing Links technique (DLX)",
  "keywords": [
    "dancing links",
    "dlx",
    "exact cover",
    "algorithm"
  ],
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "compile": "webpack",
    "uglify": "uglifyjs lib/index.js -o lib/index.min.js --mangle",
    "build": "npm run compile && npm run uglify",
    "test": "npm run build && mocha --require babel-core/register",
    "prepublish": "npm run build",
    "eslint": "eslint ."
  },
  "author": "Jonathan Taylor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorjg/dlxlibjs.git"
  },
  "homepage": "https://github.com/taylorjg/dlxlibjs",
  "bugs": {
    "url": "https://github.com/taylorjg/dlxlibjs/issues"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-register": "^6.24.0",
    "babel-runtime": "^6.23.0",
    "webpack": "^2.3.3"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "mocha": "^6.1.4",
    "uglify-js": "^3.5.15"
  }
}
