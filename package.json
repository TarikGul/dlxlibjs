{
  "name": "dlxlib",
  "version": "1.0.3",
  "description": "Solves exact cover problems by implementing Donald E. Knuth's Algorithm X using the Dancing Links technique (DLX)",
  "keywords": [
    "dancing links",
    "dlx",
    "exact cover",
    "algorithm"
  ],
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "uglify": "uglifyjs lib/index.js -o lib/index.min.js --mangle",
    "build": "webpack && npm run uglify",
    "test": "mocha --require @babel/polyfill --require @babel/register",
    "prepublish": "npm run build",
    "eslint": "eslint ."
  },
  "author": "Jonathan Taylor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorjg/dlxlibjs.git"
  },
  "homepage": "https://github.com/taylorjg/dlxlibjs",
  "bugs": {
    "url": "https://github.com/taylorjg/dlxlibjs/issues"
  },
  "devDependencies": {
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "mocha": "^6.1.4",
    "uglify-js": "^3.5.15",
    "webpack": "^4.32.1"
  }
}
