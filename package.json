{
  "name": "dlxlib",
  "version": "1.0.1",
  "description": "Solves exact cover problems by implementing Donald E. Knuth's Algorithm X using the Dancing Links technique",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "compile": "webpack",
    "uglify": "uglifyjs lib/index.js -o lib/index.min.js --mangle",
    "build": "npm run compile && npm run uglify",
    "test": "npm run build && mocha --compilers js:babel-register",
    "prepublish": "npm run build"
  },
  "author": "Jonathan Taylor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorjg/dlxlibjs.git"
  },
  "homepage": "https://github.com/taylorjg/dlxlibjs",
  "bugs": {
    "url": "https://github.com/taylorjg/dlxlibjs/issues"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-register": "^6.24.0",
    "babel-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "mocha": "^3.2.0",
    "uglify-js": "^2.8.21",
    "webpack": "^2.3.3"
  }
}
